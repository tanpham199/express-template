{
  "name": "express-template",
  "version": "1.0.1",
  "private": true,
  "description": "A minimally opinionated typescript & express starter",
  "license": "UNLICENSED",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint-staged": "lint-staged",
    "start": "yarn build && cross-env NODE_ENV=production dotenv -e .env node dist/src/index.js",
    "test": "cross-env NODE_ENV=test dotenv -e .env jest",
    "test:watch": "cross-env NODE_ENV=test dotenv -e .env jest --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^8.1.3",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.11.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.19.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  }
}
